<h1>Welcome, <%= current_user.username %></h1>
<div class="user-logout">
  <%= link_to 'Log out', logout_path %>
</div>
<%@gifs.each do |gif| %>
  <img src="<%= gif.image_path%>" alt="picture">
  <%= link_to 'Unfavorite', favorite_path(current_user.favorites.find_by(gif_id: gif.id)), method: 'delete' %>
<%end%>
