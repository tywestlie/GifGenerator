<h1>Gifs</h1>
<% if current_admin? %>
<%= link_to 'Add New Gif', new_admin_gif_path %>
<% end %>

<% @gifs.each do |gif| %>
  <div id="gif-<%= gif.id%>">
    <img  src='<%= gif.image_path %>' alt='<%= gif.id %>'>
    <%= link_to "Gif-#{gif.id}", gif_path(gif) %>
    <%= link_to 'Delete', admin_gif(gif), method: delete %>
  </div>
  <div class="favorite">

  </div>
<%end%>
